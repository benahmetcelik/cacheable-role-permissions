{
  "name": "benahmetcelik/cacheable-permissions",
  "description": "Laravel için önbelleklenebilir permission ve role trait'leri",
  "type": "library",
  "license": "MIT",
  "keywords": ["laravel", "permissions", "cache", "roles", "spatie"],
  "authors": [
    {
      "name": "Ahmet ÇELİK",
      "email": "ben4hmetcelik@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0",
    "illuminate/cache": "^10.0|^11.0",
    "spatie/laravel-permission": "^5.0|^6.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0|^9.0",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Cacheable\\CacheablePermissions\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cacheable\\CacheablePermissions\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Cacheable\\CacheablePermissions\\CacheablePermissionsServiceProvider"
      ],
      "aliases": {
        "CacheablePermissions": "Cacheable\\CacheablePermissions\\Facades\\CacheablePermissions"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}